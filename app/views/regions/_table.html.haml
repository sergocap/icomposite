.regions
  %table{ class: 'regions_table' }
    - (0..regions.maximum(:y)).each do |y|
      %tr{class: 'tr1'}
        - (0..regions.maximum(:x)).each do |x|
          %td{class: 'td1'}
            .region
              - region = regions.where(:x => x, :y => y).first
              = link_to image_tag(region.preview.url(:medium)), project_region_path(region.project.id, region.id)

