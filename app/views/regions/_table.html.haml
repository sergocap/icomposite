.regions
  %table
    - (0..regions.maximum(:y)).each do |y|
      %tr
        - (0..regions.maximum(:x)).each do |x|
          %td
            .region
              - region = regions.where(:x => x, :y => y).first
              = link_to image_tag(region.preview.url), project_region_path(region.project.id, region.id)

