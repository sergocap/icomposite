.complete_project_medium
  %table
    - (0..project.regions.maximum(:y)).each_with_index do |y, index_y|
      %tr
        - (0..project.regions.maximum(:x)).each_with_index do |x, index_x|
          - region = project.regions.where(:x => index_x, :y => index_y).first
          %td{ width: region.medium_width.ceil, height: region.medium_height.ceil  }
            = render partial: 'places/complete_table_medium', locals: { :places => region.places, :region => region, :project => project, :size_place_x_medium => (project.size_place_x / project.resize_factor).to_i, :size_place_y_medium => (project.size_place_y / project.resize_factor).to_i }
